option_settings:
  - namespace: aws:elb:policies
    option_name: ConnectionSettingIdleTimeout
    value: 3600
  - namespace: aws:elasticbeanstalk:command
    option_name: Timeout
    value: 3600

commands:
    numpy_install:
        command: sudo python3 -m pip install numpy
        cwd: /home/
    surprise_install:
        command: sudo python3 -m pip install scikit-surprise
        cwd: /home/
    spacy_install:
        command: sudo python3 -m pip install spacy
        cwd: /home/
    spacy_lg_model_install:
        command: sudo python3 -m spacy install scikit-surprise